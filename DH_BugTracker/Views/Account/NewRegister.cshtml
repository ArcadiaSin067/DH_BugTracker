@{
    ViewBag.Title = "New User Registration";
    Layout = "~/Views/Shared/_LoginShared.cshtml";
}

<span class="login100-form-title p-b-33">
    @ViewBag.Title
</span>
<div class="row">
    <div class="col-lg-8 col-md-10 mx-auto d-flex justify-content-center">
        Thank you for registering. Be on the lookout for a confirmation email,
        and when you receive the email click the link and log in!
    </div>
</div>
